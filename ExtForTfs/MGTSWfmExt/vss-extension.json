    "contributions": [
        {
          "id": "iisStSt",
          "type": "ms.vss-distributed-task.task",
          "targets": [
            "ms.vss-distributed-task.tasks"
          ],
          "properties": {
            "name": "Tasks/iisStSt1"
          }
        },
        {
            "id": "jsonTransf",
            "type": "ms.vss-distributed-task.task",
            "targets": [
              "ms.vss-distributed-task.tasks"
            ],
            "properties": {
              "name": "Tasks/jsonTransf1"
            }
          }
    ],
   "repository": {
        "type": "git",
        "uri": "https://github.com/ShaAnatoliy/ExtForTfs"
    },
	"tags": [
        "IIS stop site",
        "IIS start site",
        "JSON",
        "Transform file"
    ],
	"screenshots": [
        {
            "path": "images/scsh1.png"
        }
    ],
	 "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "License.txt"
        }
    },
	"branding": {
        "color": "rgb(0, 128, 192)",
        "theme": "dark"
    }

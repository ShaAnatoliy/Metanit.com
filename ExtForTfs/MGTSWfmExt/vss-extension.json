{
    "manifestVersion": 1,
    "id": "MGTSWfmExt",
    "name": "МГТС. Расширения для TFS",
    "version": "1.0.4.17",
    "publisher": "ShelganovAE",
    "description": "MGTS WFM ext for TFS",
    "targets": [
        {
            "id": "Microsoft.TeamFoundation.Server"
        }
    ],
    "categories": [
        "Build and release"
    ],
    "visibility": [
        "Build",
        "Release"
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
	    "files": [
        {
            "path": "Tasks/iisStSt1"
        },
        {
            "path": "Tasks/jsonTransf1"
